{
    "compilerOptions": 
    {
        "target": "ES6",
        "jsx": "react",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitReturns":true,
        "noImplicitThis":true,
        "outDir": "dist"
    },
    "include": 
    [
        "src/**/*.ts",
        "src/**/*.tsx"
    ],
    "exclude": 
    [
        "dist"
    ]
}